{
  "name": "worldInMe",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development node ./bin/www",
    "start-prod": "NODE_ENV=production node ./bin/www",
    "build": "NODE_ENV=development gulp",
    "build-prod": "NODE_ENV=production gulp build-prod",
    "deploy": "bin/deploy",
    "test": "babel-tape-runner client/tests/**/*.js | faucet"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "build": {
    "source": "./client",
    "destination": "./public",
    "scripts": {
      "source": "./client/main.js",
      "destination": "./public/js/",
      "extensions": [
        "js",
        "jsx"
      ],
      "filename": "bundle.js"
    }
  },
