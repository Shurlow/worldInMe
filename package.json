    "chance": "^0.8.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "draft-js": "^0.7.0",
    "es6-promise": "^3.0.2",
    "express": "~4.13.0",
    "express-jwt": "^3.3.0",
